{
  "name": "w-api",
  "version": "1.0.0",
  "description": "boilerplate for api",
  "main": "app.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/app.js",
    "watch-node": "nodemon dist/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "test": "npm run test",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vildantursic/w-api.git"
  },
  "keywords": [
    "api",
    "expressjs",
    "nodejs"
  ],
  "author": " <vildantursic@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vildantursic/w-api/issues"
  },
  "homepage": "https://github.com/vildantursic/w-api#readme",
  "dependencies": {
    "express": "^4.16.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/body-parser": "^1.16.8",
    "nodemon": "^1.17.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "ts-node": "^5.0.0"
  }
}
